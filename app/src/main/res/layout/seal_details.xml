<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:gravity="top"
    android:orientation="vertical"

    tools:context="com.fedco.mbc.activity.Readinginput">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/Header"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_alignParentTop="true"
        android:background="@color/color_primary">

        <RelativeLayout
            android:id="@+id/relback"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/button_continue">

            <Button
                android:id="@+id/Buttonback"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center|right"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:background="@drawable/back"
                android:fontFamily="sans-serif"
                android:padding="7dp"
                android:textColor="@drawable/button_login_textcolor"
                android:textStyle="bold" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relButton"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"

            android:background="@drawable/button_continue">

            <Button
                android:id="@+id/ButtonContreading"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:fontFamily="sans-serif"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center|right"
                android:layout_marginRight="13dp"
                android:background="@drawable/forward"

                android:padding="7dp" />
        </RelativeLayout>

        <TextView
            android:id="@+id/TextViewHEADER"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:padding="8dp"
            android:text="@string/seal_dt"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="14dp"
            android:fontFamily="sans-serif"
            android:textStyle="bold" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="430dp"
        android:layout_below="@+id/Header">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/login_tab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:background="@color/color_primary"
                android:shrinkColumns="*"
                android:stretchColumns="*">

                <TextView
                    android:id="@+id/Headertext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="@string/od_seal"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFF"
                    android:fontFamily="sans-serif"
                    android:textStyle="bold" />


                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/OSOBS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:fontFamily="sans-serif"
                        android:padding="10dp"

                        android:text="@string/ou_box_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/OSOBSValues"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:fontFamily="sans-serif"
                        android:padding="10dp"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"

                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/OSIBS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/in_box_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/OSIBSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:background="@color/color_primary"
                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/OSOS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/opt_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/OSOSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:background="@color/color_primary"
                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/OSMBRS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/md_btn_re_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/OSMBRSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

                        android:maxLength="10"
                        android:fontFamily="sans-serif"
                        android:padding="10dp"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>


            </TableLayout>

            <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/login_tab2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:background="@color/color_primary"
                android:shrinkColumns="*"
                android:stretchColumns="*">

                <TextView
                    android:id="@+id/Headertext2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="sans-serif"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="@string/nw_seal"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />


                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/NSOBS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/ou_box_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/NSOBSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"

                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/NSIBS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/in_box_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/NSIBSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:background="@color/color_primary"
                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/NSOS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/opt_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/NSOSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />
                </TableRow>


                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="2dp"
                    android:background="@color/color_primary"
                    android:gravity="center_horizontal">

                    <TextView
                        android:id="@+id/NSMBRS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:padding="10dp"
                        android:fontFamily="sans-serif"
                        android:text="@string/md_btn_re_seal"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#cccccc" />

                    <EditText
                        android:id="@+id/NSMBRSValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                        android:maxLength="10"
                        android:padding="10dp"
                        android:singleLine="true"
                        android:fontFamily="sans-serif"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff" />

                </TableRow>

            </TableLayout>
        </LinearLayout>
    </ScrollView>


</RelativeLayout>
