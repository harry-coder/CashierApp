<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"

    android:gravity="top"
    android:orientation="vertical">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/Footer"
        style="@style/header_thm">

        <TextView
            android:id="@+id/TextViewHe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center|right"
            android:layout_margin="5dp"
            android:padding="10dp"
            android:text="@string/feed_sur"
            android:textColor="#ffffff"
            android:textStyle="bold" />

        <RelativeLayout
            android:id="@+id/relback"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/back_new">

            <Button
                android:id="@+id/Buttonback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center|right"
                android:background="@drawable/back_selector"
                android:paddingLeft="10dp"
                android:text="@string/bk"
                android:textColor="#FFF"
                android:textSize="10dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relButton"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/front_new">

            <Button
                android:id="@+id/ButtonContreading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center|left"
                android:background="@drawable/forward_selector"
                android:paddingRight="10dp"
                android:text="@string/view_next"
                android:textColor="#FFF"
                android:textSize="10dp" />
        </RelativeLayout>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Footer">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/login_tab"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="7dp"
            android:orientation="vertical"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/picture_frame"
                android:orientation="vertical"
                android:padding="10dp"
                android:paddingBottom="20dp">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:shadowDx="1"
                    android:shadowDy="1"
                    android:shadowRadius="1"
                    android:text="@string/geo"
                    android:textColor="@android:color/secondary_text_dark_nodisable"
                    android:textSize="14sp" />

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_33kv_feedername"
                        style="@style/textview_gen"
                        android:text="@string/divi"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <Spinner
                        android:id="@+id/spr_division"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:paddingLeft="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/dc_row"
                    style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_dc"
                        style="@style/textview_gen"
                        android:text="@string/dc"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <Spinner
                        android:id="@+id/spin_dc"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:background="@android:drawable/picture_frame"
                android:orientation="vertical"
                android:padding="10dp"
                android:paddingBottom="20dp">

                <TextView

                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:shadowDx="1"
                    android:shadowDy="1"
                    android:shadowRadius="1"
                    android:text="@string/ele"
                    android:textColor="@android:color/secondary_text_dark_nodisable"
                    android:textSize="14sp" />

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_33_11kv_substation"
                        style="@style/textview_gen"
                        android:text="@string/sub_st"

                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <AutoCompleteTextView
                        android:id="@+id/spinner_33_11kv_substation"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:hint="@string/st_typ"
                        android:inputType="text"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/TV_FEEDER_NAME"
                        style="@style/textview_gen"
                        android:text="11 kV Feeder Name :"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <Spinner
                        android:id="@+id/spr_11kv_feedername"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text" />
                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_11kv_feedername"
                        style="@style/textview_gen"
                        android:text="@string/add_feed_name"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_11kv_feedername"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="text"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout
                    style="@style/linear_fieldset"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <LinearLayout
                        style="@style/linear_fieldset"
                        android:layout_weight="12"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_pole_number"
                            style="@style/textview_gen"
                            android:text="@string/tap_point"
                            android:textAppearance="?android:attr/textAppearanceSmall" />

                        <TextView
                            android:id="@+id/tv_tap_point"
                            style="@style/edittext_gen_stl"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="@drawable/edit_text"
                            android:gravity="left|center"
                            android:hint="Auto Generated Tap Point No."
                            android:inputType="text"
                            android:textAppearance="?android:attr/textAppearanceSmall" />

                        <Spinner
                            android:id="@+id/spin_tap_point"
                            style="@style/edittext_gen_stl"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="@drawable/edit_text" />
                    </LinearLayout>

                    <LinearLayout
                        style="@style/linear_fieldset"
                        android:layout_width="wrap_content"

                        android:layout_gravity="center"
                        android:layout_marginTop="15dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_new"
                            android:layout_width="50dp"
                            android:layout_height="47dp"
                            android:text="N" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:background="@android:drawable/picture_frame"
                android:orientation="vertical"
                android:padding="10dp"
                android:paddingBottom="20dp">

                <TextView

                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:shadowDx="1"
                    android:shadowDy="1"
                    android:shadowRadius="1"
                    android:text="@string/survey_meter"
                    android:textColor="@android:color/secondary_text_dark_nodisable"
                    android:textSize="14sp" />

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_meter_status"
                        style="@style/textview_gen"
                        android:text="@string/mtr_st_status"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <Spinner
                        android:id="@+id/spr_meter_status"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:entries="@array/meter_status_array" />
                </LinearLayout>

                <LinearLayout
                    style="@style/linear_fieldset"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/tv_feeder_location"
                        style="@style/textview_gen"
                        android:text="@string/feed_loc"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_feeder_location"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="text"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>


                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_meter_number"
                        style="@style/textview_gen"
                        android:text="@string/mtr_number"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_meter_number"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="text"
                        android:maxLength="12"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">


                    <LinearLayout
                        style="@style/linear_fieldset"
                        android:orientation="horizontal"
                        android:weightSum="5">

                        <TextView
                            android:id="@+id/tv_meter_com_port"
                            style="@style/textview_gen"
                            android:layout_weight="4"
                            android:text="@string/mtr_com_port"
                            android:textAppearance="?android:attr/textAppearanceSmall" />

                        <Switch
                            android:id="@+id/cb_com_port"
                            style="@style/edittext_gen_stl"
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:layout_weight="1" />
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="9"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <CheckBox
                            android:id="@+id/chk_amr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:text="@string/amr"
                            android:textSize="9dp" />

                        <CheckBox
                            android:id="@+id/chk_optical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:text="@string/opt"
                            android:textSize="9dp" />

                        <CheckBox
                            android:id="@+id/chk_lpr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:text="@string/lpr"
                            android:textSize="9dp" />

                        <CheckBox
                            android:id="@+id/chk_na"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:text="@string/na"
                            android:textSize="9dp"
                            android:visibility="gone" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    style="@style/linear_fieldset"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/tv_meter_reading"
                        style="@style/textview_gen"
                        android:text="@string/mtr_reading"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_meter_reading"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="numberDecimal"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_meter_box_status"
                        style="@style/textview_gen"
                        android:text="@string/mtr_box_status"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <Spinner
                        android:id="@+id/spr_box_status"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:entries="@array/box_status"
                        android:paddingLeft="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSealstatus"
                    style="@style/linear_fieldset">


                    <RelativeLayout
                        android:id="@+id/linearLayout12"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="right"
                        android:layout_weight="9"
                        android:orientation="horizontal"
                        android:paddingRight="10dp"
                        android:paddingTop="10dp">

                        <TextView
                            android:id="@+id/tv_box_seal_text"
                            style="@style/textview_gen"
                            android:text="@string/mtr_box_sl_status"
                            android:textAppearance="?android:attr/textAppearanceSmall" />

                        <TextView
                            android:id="@+id/tv_box_seal_status"
                            style="@style/textview_gen"
                            android:layout_width="wrap_content"
                            android:layout_toLeftOf="@+id/chk_seal"
                            android:layout_toStartOf="@+id/chk_seal"
                            android:text="@string/open"
                            android:textAppearance="?android:attr/textAppearanceSmall" />

                        <Switch
                            android:id="@+id/chk_seal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/tv_box_seal_txt"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_gravity="right"
                            android:padding="5dp"
                            android:textSize="9dp" />

                    </RelativeLayout>

                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_meter_make"
                        style="@style/textview_gen"
                        android:text="@string/mtr_make"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <Spinner
                        android:id="@+id/spr_meter_make"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text" />

                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_mf"
                        style="@style/textview_gen"
                        android:text="@string/mf"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_mf_reading"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="numberDecimal"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_CT_"
                        style="@style/textview_gen"
                        android:text="@string/ctr"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_ct_reading"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="text"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/tv_pt"
                        style="@style/textview_gen"
                        android:text="@string/ptr"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/et_pt_reading"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/edit_text"
                        android:inputType="text"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:background="@android:drawable/picture_frame"
                android:orientation="vertical"
                android:padding="10dp"
                android:paddingBottom="20dp">

                <TextView

                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:shadowDx="1"
                    android:shadowDy="1"
                    android:shadowRadius="1"
                    android:text="@string/observation"
                    android:textColor="@android:color/secondary_text_dark_nodisable"
                    android:textSize="14sp" />

                <LinearLayout style="@style/linear_fieldset">

                    <TextView
                        android:id="@+id/TextViewRIPf"
                        style="@style/textview_gen"
                        android:text="@string/rmk"
                        android:textAppearance="?android:attr/textAppearanceSmall" />

                    <EditText
                        android:id="@+id/TextViewRIPfValue"
                        style="@style/edittext_gen_stl"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:background="@drawable/edit_text"
                        android:gravity="top"
                        android:inputType="text"
                        android:paddingLeft="10dp"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </ScrollView>


</RelativeLayout>
