<resources>
    <attr name="direction">
        <enum name="rightToLeft" value="0" />
        <enum name="bottomToTop" value="1" />
        <enum name="leftToRight" value="2" />
        <enum name="topToBottom" value="3" />
    </attr>


    <style name="AppBaseTheme.Printer" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>


    <declare-styleable name="MultiDirectionSlidingDrawer">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="direction" />
        <attr name="bottomOffset" format="dimension"  />
        <attr name="topOffset" format="dimension"  />
        <attr name="allowSingleTap" format="boolean" />
        <attr name="animateOnClick" format="boolean" />
    </declare-styleable>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary1</item>
        <item name="colorAccent">@color/color_accent</item>
        <item name="homeAsUpIndicator">@drawable/back</item>
        <!--<item name="android:spinnerDropDownItemStyle">@style/mySpinnerItemStyle</item>-->
    </style>

    <!-- Base application theme. -->
    <style name="AppThemeSurvey" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary_blue</item>
        <item name="colorPrimaryDark">@color/color_primary_blue_light</item>
        <item name="colorAccent">@color/orange5</item>
        <item name="homeAsUpIndicator">@drawable/back</item>
        <!--<item name="android:spinnerDropDownItemStyle">@style/mySpinnerItemStyle</item>-->
    </style>

    <!-- Base application theme. -->
    <style name="AppThemeSurveyMain" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary_blue</item>
        <item name="colorPrimaryDark">@color/color_primary_blue_light</item>
        <item name="colorAccent">@color/orange5</item>
        <item name="homeAsUpIndicator">@drawable/back</item>
        <!--<item name="android:spinnerDropDownItemStyle">@style/mySpinnerItemStyle</item>-->
    </style>

    <!-- Base application theme. -->
    <style name="DialogeAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary1</item>
        <item name="colorAccent">@color/color_accent</item>
        <item name="homeAsUpIndicator">@drawable/back</item>
        <!--<item name="android:spinnerDropDownItemStyle">@style/mySpinnerItemStyle</item>-->
    </style>

    <!-- Base application theme. -->
    <style name="DialogeValTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary1</item>
        <item name="colorAccent">@color/color_accent</item>
        <item name="homeAsUpIndicator">@drawable/back</item>
        <!--<item name="android:spinnerDropDownItemStyle">@style/mySpinnerItemStyle</item>-->
    </style>

    <style name="CustomProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custombar</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>

    <style name="TextAppearance.YourApp.FloatLabel" parent="android:TextAppearance.Small">
        <item name="android:textColor">@color/color_primary</item>
        <item name="android:textSize">12sp</item>
    </style>

    <!--<style name="mySpinnerItemStyle" parent="@android:style/Widget.Holo.DropDownItem.Spinner">
        <item name="android:textColor">#FFF</item>
    </style>-->

    <style name="Theme.AppCompat.Light.NoActionBar" parent="@style/Theme.AppCompat.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item> <!-- For 2.x version  -->
    </style>

    <style name="AppBaseTheme" parent="android:Theme.Light">

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="textfont" parent="@android:style/TextAppearance.Medium">
        <item name="android:typeface">sans</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="MenuFragmentStyle" parent="AppTheme">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:colorForeground">@android:color/transparent</item>
        <item name="android:colorForegroundInverse">@android:color/transparent</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="MyMaterialTheme.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary1</item>
        <item name="colorAccent">@color/color_primary</item>
    </style>
    <style name="header_thm">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/gradient</item>
    </style>

    <style name="textview_gen">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">#3E4A7B</item>
        <item name="android:padding">4dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:textSize">14dp</item>
    </style>
    <style name="edittext_gen_stl">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="linear_fieldset">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>


    <style name="cam_img_box2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="cam_img_box2in">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/cam_bg2</item>
        <item name="android:minHeight">230dp</item>
    </style>
    <style name="cam_box2_tex">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14dp</item>
        <item name="android:typeface">normal</item>
        <item name="android:textAlignment">center</item>

    </style>

    <style name="cam_box2_latlong_ttl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">2dp</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:typeface">normal</item>

    </style>
    <style name="cam_box2_latlong_det">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">2dp</item>
        <item name="android:textSize">10dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:typeface">normal</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="spinner_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/gradient_spinner</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:popupBackground">#DFFFFFFF</item>
    </style>

</resources>
